{
  "scripts": {
    "start": "cd contacts && npm start",
    "server": "cd contactServer && node ./bin/www",
    "client": "cd contacts && npm start --prefix contacts",
    "clientinstall": "npm install --prefix contacts",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "render-postbuild": "NPM_CONFIG_PRODUCTION=false cd contacts && npm install --prefix contacts && npm run build -- prefix contacts"
  },
  "dependencies": {
    "axios": "^1.2.2",
    "create-react-app": "^5.0.1"
  }
}
